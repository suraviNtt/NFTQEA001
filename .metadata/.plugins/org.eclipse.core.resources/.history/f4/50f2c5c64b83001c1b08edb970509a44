package intro;

public class B extends A {

	public static void main(String[] args) {
		A a = new A();
		a.test();
		
		B b = new B();
		b.test();
		
		A a1 = new B();
		a1.test();
		a1.test2();;
		
	}
	
	public void test() {
		System.out.println("Test B");
	}
	
	public void test2() {
		System.out.println("Test B2");
	}
	
}
